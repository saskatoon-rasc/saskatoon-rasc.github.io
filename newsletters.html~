function button_on ( imgName )

{

        if ( version == "n3" || version == "e4" )

        {

                butOn = eval ( imgName + "_on.src" );

                document [imgName].src = butOn;

        }

}



function button_off ( imgName )

{

        if ( version == "n3" || version == "e4" )

        {

                butOff = eval ( imgName + "_off.src" );

                document [imgName].src = butOff;

        }

}

// --></script>


